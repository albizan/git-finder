{
  "name": "git-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-spinners": "^0.5.8",
    "styled-components": "^4.3.2",
    "tailwindcss": "^1.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm-run-all -s build:css start",
    "build": "npm-run-all -s build:css && react-scripts build",
    "watch:css": "postcss src/assets/css/tailwind.css -o src/assets/css/index.css -w",
    "build:css": "postcss src/assets/css/tailwind.css -o src/assets/css/index.css"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "eslint-plugin-prettier": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.18.2"
  }
}
